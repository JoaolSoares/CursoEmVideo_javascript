[1mdiff --git a/aula12ex/ex015/script.js b/aula12ex/ex015/script.js[m
[1mnew file mode 100644[m
[1mindex 0000000..36bbf2a[m
[1m--- /dev/null[m
[1m+++ b/aula12ex/ex015/script.js[m
[36m@@ -0,0 +1,40 @@[m
[32m+[m[32mfunction verificar() {[m
[32m+[m[32m    var data = new Date()[m
[32m+[m[32m    var ano = data.getFullYear()[m
[32m+[m[32m    var fano = window.document.getElementById("txtano")[m
[32m+[m[32m    var res = window.document.querySelector("div#res")[m
[32m+[m
[32m+[m[32m    if (fano.value.length == 0 || fano.value > ano) {[m
[32m+[m[32m        window.alert("ERRO! Verifique os dados e tente novamente")[m
[32m+[m[32m    }[m
[32m+[m[32m    else {[m
[32m+[m[32m        var fsex = document.getElementsByName("radsex")[m
[32m+[m[32m        var idade = ano - Number(fano.value)[m
[32m+[m[32m        var genero = ""[m
[32m+[m
[32m+[m[32m        // cria uma img no html s√≥ que por js[m
[32m+[m[32m        var img = document.createElement("img")[m
[32m+[m[32m        img.setAttribute("id", "foto")[m
[32m+[m
[32m+[m[32m        if (fsex[0].checked) {[m
[32m+[m[32m            genero = "homem"[m
[32m+[m[32m            if (idade >= 0 && idade < 50) {[m
[32m+[m[32m                img.setAttribute("src", "foto_jovemo.jpg")[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                img.setAttribute("src", "foto_idoso.jpg")[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (fsex[1].checked) {[m
[32m+[m[32m            genero = "Mulher"[m
[32m+[m[32m            if (idade >= 0 && idade < 50) {[m
[32m+[m[32m                img.setAttribute("src", "foto_jovema.jpg")[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                img.setAttribute("src", "foto_idosa.jpg")[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        res.innerHTML = `Voce √© um/a ${genero} com ${idade} anos`[m
[32m+[m[32m        res.appendChild(img)[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
