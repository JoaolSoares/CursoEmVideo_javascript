[32m+[m[32m    <p>Primeiros passos em <strong>DOM</strong></p>[m
[32m+[m[32m    <div id='msg'>Clique aqui</div>[m
[32m+[m
[32m+[m[32m    <script>[m
[32m+[m[32m        document.write('</br></br>Apartir daqui √© js:</br></br>')[m
[32m+[m
[32m+[m[32m        // da um get no itenm [0] com a tag p no HTML[m
[32m+[m[32m        var p1 = window.document.getElementsByTagName('p')[0][m
[32m+[m[32m        // reescreve todo o html da tag p[m
[32m+[m[32m        window.document.write(p1.innerHTML + '</br>')[m
[32m+[m[32m        // muda o cor de uma tag p do html[m
[32m+[m[32m        p1.style.color = 'black'[m
[32m+[m
[32m+[m[32m        // escreve a URL do site[m
[32m+[m[32m        window.document.write(window.document.URL + '</br>')[m
[32m+[m
[32m+[m[32m        // da um get nos elementos com o ID 'msg' do HTML[m
[32m+[m[32m        var d = document.getElementById('msg')[m
[32m+[m[32m        // reescreve o HTML com o ID 'msg' que foi guardada na var acima[m
[32m+[m[32m        document.write(d.innerText)[m
[32m+[m
[32m+[m[32m        // muda a cor do fundo por DOM[m
[32m+[m[32m        window.document.body.style.background = 'crimson'[m
[32m+[m[32m    </script>[m
[32m+[m
[32m+[m[32m</body>[m
[32m+[m[32m</html>[m
